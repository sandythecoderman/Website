{
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp",
        "--port",
        "3000",
        "--headless",
        "--viewport-size",
        "1280,720",
        "--browser",
        "chromium"
      ],
      "env": {
        "NODE_ENV": "development"
      }
    }
  }
} 